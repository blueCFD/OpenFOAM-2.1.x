
/*neutralize unwanted lib includes...*/
PROJECT_LIBS =

#if defined( WIN32 ) || defined( WIN64 )

sinclude $(GENERAL_RULES)/mplib$(WM_MPLIB)
sinclude $(RULES)/mplib$(WM_MPLIB)

EXE_INC = -I$(OBJECTS_DIR) \
    $(PFLAGS) $(PINC)

