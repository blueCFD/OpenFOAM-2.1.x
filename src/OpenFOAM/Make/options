           -L$(FOAM_LIBBIN)/$(FOAM_MPI)/ -lPstream \
           -lz \
           -lregex \
           -lstack_trace \
           $(PLIBS)

#else

EXE_INC = -I$(OBJECTS_DIR)

LIB_LIBS = \
    $(FOAM_LIBBIN)/libOSspecific.o \
    -L$(FOAM_LIBBIN)/dummy -lPstream \
    -lz

#endif
